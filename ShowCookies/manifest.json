{
    "name": "ShowCookies",
    "version": "1.0",
    "description": "__MSG_showThis_description__",
    "icons": {
        "16": "img/icon_16x16.png",
        "48": "img/icon_48x48.png",
        "128": "img/icon_128x128.png"
    },
    "default_locale": "tr",
    "browser_action": {
        "default_icon": {
            "19": "img/icon_19x19.png",
            "38": "img/icon_38x38.png"
        },
        "default_title": "ShowCookies",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": [
            "/lib/jquery-3.3.1.min.js",
            "/lib/object-watch.js",
            "/js/cookie_helpers.js",
            "/js/utils.js",
            "/js/data.js",
             "/js/background.js",
            "/js/ga.js",
            "db.js"
        ]
    },
    "options_page": "options_main_page.html",
    "permissions": [
        "tabs",
        "\u003Call_urls\u003E",
        "cookies",
        "contextMenus",
        "unlimitedStorage",
        "notifications",
        "storage",
        "clipboardWrite"
    ],
    "manifest_version": 2
}
